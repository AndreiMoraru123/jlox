class Cake {

  init(adjective) {
    this.adjective = adjective;
  }

  taste() {
    print "The " + this.flavor + " cake is " + this.adjective + "!";
  }
}

extend Cake {
  cook () {
    print "Mmm delicious";
  }
}

var cake = Cake("delicious");
cake.flavor = "German chocolate";
cake.taste();
cake.cook();
